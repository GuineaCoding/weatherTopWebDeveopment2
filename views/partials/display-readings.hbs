<table class="table is-narrow is-hoverable is-fullwidth">
  <thead>
    <tr>
      <th><abbr title="Code">Code</abbr></th>
      <th><abbr title="Temp">Temp</abbr></th>
      <th><abbr title="Wind Speed">Wind Speed</abbr></th>
      <th><abbr title="Pressure">Pressure</abbr></th>
    </tr>
  </thead>
  <tbody>
    {{#each readings}}
    <tr>
      <td>{{code}}</td>
      <td>{{temperature}}</td>
      <td>{{windSpeed}}</td>
      <td>{{pressure}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
