<table class="table is-narrow is-hoverable is-fullwidth">
  <thead>
    <tr>
      <th>
        <p>Time/Date</p>
      </th>
      <th>
        <p title="Code">Code</p>
      </th>
      <th>
        <p title="Temp">Temp</p>
      </th>
      <th>
        <p title="Wind Speed">Wind Speed</p>
      </th>
      <th>
        <p title="Wind Direction">Wind Direction</p>
      </th>
      <th>
        <p title="Pressure">Pressure</p>
      </th>
      <th>
        <p><title="Delete">Delete</title></p>
      </th>
    </tr>
  </thead>
  <tbody>
    {{#each station.readings}}
    <tr>
      <td>{{date}}</td>
      <td>{{code}}</td>
      <td>{{temperature}}</td>
      <td>{{windSpeed}}</td>
      <td>{{windDirection}}</td>
      <td>{{pressure}}</td>
      <td>Delete</td>
    </tr>
    {{/each}}
  </tbody>
</table>